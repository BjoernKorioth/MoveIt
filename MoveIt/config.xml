<?xml version='1.0' encoding='utf-8'?>
<widget id="com.moveitproject.www" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MoveIt!</name>
    <description>An new awesome health app.</description>
    <author email="hi@ionicframework.com" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <resource-file src="google-services.json" target="app/google-services.json" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application" xmlns:android="http://schemas.android.com/apk/res/android">
            <application android:networkSecurityConfig="@xml/network_security_config" />
        </edit-config>
        <resource-file src="resources/android/xml/network_security_config.xml" target="app/src/main/res/xml/network_security_config.xml" />
        <allow-intent href="market:*" />
        <icon density="ldpi" src="src/assets/icon_android/Icon-36.png" />
        <icon density="mdpi" src="src/assets/icon_android/Icon-48.png" />
        <icon density="hdpi" src="src/assets/icon_android/Icon-72.png" />
        <icon density="xhdpi" src="src/assets/icon_android/Icon-96.png" />
        <icon density="xxhdpi" src="src/assets/icon_android/Icon-144.png" />
        <icon density="xxxhdpi" src="src/assets/icon_android/Icon-192.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="src/assets/icon_ios/Icon-57.png" width="57" />
        <icon height="114" src="src/assets/icon_ios/Icon-114.png" width="114" />
        <icon height="29" src="src/assets/icon_ios/Icon-29.png" width="29" />
        <icon height="58" src="src/assets/icon_ios/Icon-58.png" width="58" />
        <icon height="87" src="src/assets/icon_ios/Icon-87.png" width="87" />
        <icon height="20" src="src/assets/icon_ios/Icon-20.png" width="20" />
        <icon height="40" src="src/assets/icon_ios/Icon-40.png" width="40" />
        <icon height="60" src="src/assets/icon_ios/Icon-60.png" width="60" />
        <icon height="48" src="src/assets/icon_ios/Icon-48.png" width="48" />
        <icon height="55" src="src/assets/icon_ios/Icon-55.png" width="55" />
        <icon height="29" src="src/assets/icon_ios/Icon-29.png" width="29" />
        <icon height="58" src="src/assets/icon_ios/Icon-58.png" width="58" />
        <icon height="87" src="src/assets/icon_ios/Icon-87.png" width="87" />
        <icon height="40" src="src/assets/icon_ios/Icon-40.png" width="40" />
        <icon height="80" src="src/assets/icon_ios/Icon-80.png" width="80" />
        <icon height="120" src="src/assets/icon_ios/Icon-120.png" width="120" />
        <icon height="88" src="src/assets/icon_ios/Icon-88.png" width="88" />
        <icon height="50" src="src/assets/icon_ios/Icon-50.png" width="50" />
        <icon height="100" src="src/assets/icon_ios/Icon-100.png" width="100" />
        <icon height="60" src="src/assets/icon_ios/Icon-60.png" width="60" />
        <icon height="120" src="src/assets/icon_ios/Icon-120.png" width="120" />
        <icon height="180" src="src/assets/icon_ios/Icon-180.png" width="180" />
        <icon height="72" src="src/assets/icon_ios/Icon-72.png" width="72" />
        <icon height="144" src="src/assets/icon_ios/Icon-144.png" width="144" />
        <icon height="76" src="src/assets/icon_ios/Icon-76.png" width="76" />
        <icon height="152" src="src/assets/icon_ios/Icon-152.png" width="152" />
        <icon height="167" src="src/assets/icon_ios/Icon-167.png" width="167" />
        <icon height="172" src="src/assets/icon_ios/Icon-172.png" width="172" />
        <icon height="196" src="src/assets/icon_ios/Icon-196.png" width="196" />
        <icon height="1024" src="src/assets/icon_ios/Icon-1024.png" width="1024" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^4.0.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="cordova-plugin-health" source="npm">
        <variable name="HEALTH_READ_PERMISSION" value="App needs read access" />
        <variable name="HEALTH_WRITE_PERMISSION" value="App needs write access" />
        <variable name="GMS_VERSION" value="16.0.1" />
    </plugin>
    <config-file parent="NSHealthShareUsageDescription" platform="ios">
        <string>App needs read access</string>
    </config-file>
    <config-file parent="NSHealthUpdateUsageDescription" platform="ios">
        <string>App needs write access</string>
    </config-file>
</widget>
